<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="couponsmainact.cashcoupons">

    <application
        android:allowBackup="true"
        android:icon="@drawable/coupons_app_icon"
        android:label="@string/app_name_coupons"
        android:supportsRtl="true"
        android:theme="@style/ActionBarSupport">
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CouponsListActivity"
            android:label="@string/title_activity_coupons_list"></activity>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/action_settings"></activity>

        <activity android:name=".CouponDetailActivity"
            android:label="CouponDetail"></activity>
    </application>

</manifest>
